{
  "name": "vuetom-cli",
  "version": "0.1.3",
  "description": "A simple CLI for vitepress-theme-vuetom",
  "scripts": {
    "cli": "node ./bin/vuetom-cli.js",
    "cli:init": "node ./bin/vuetom-cli.js init",
    "cli:list": "node ./bin/vuetom-cli.js list",
    "tsc": "npm run clean && tsc",
    "clean": "rimraf bin",
    "eslint": "eslint src --ext .ts",
    "pub": "npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "vuetom-cli": "./bin/vuetom-cli.js"
  },
  "files": [
    "bin/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lauset/vuetom-cli.git"
  },
  "keywords": [
    "vitepress",
    "theme",
    "vue",
    "cli"
  ],
  "author": "lauset",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lauset/vuetom-cli/issues"
  },
  "homepage": "https://github.com/lauset/vuetom-cli#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "download-git-repo": "^3.0.2",
    "inquirer": "^8.2.0",
    "ora": "^5.4.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^8.1.3",
    "@types/json-schema": "^7.0.9",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^17.0.10",
    "@types/request": "^2.48.8",
    "@types/unist": "^2.0.6",
    "@types/vfile": "^3.0.2",
    "@types/vfile-message": "^1.0.1",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-plugin-typescript": "^0.14.0",
    "typescript": "^4.5.5"
  }
}
